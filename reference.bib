	file = {Full Text PDF:/Users/gayoungp/Zotero/storage/GWNW57SG/González-Ruibal - 2016 - Ethnoarchaeology or simply archaeology.pdf:application/pdf;Snapshot:/Users/gayoungp/Zotero/storage/EW227K49/00438243.2016.html:text/html}
