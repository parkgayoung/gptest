}

@article{quintero_evolution_1995,
	title = {Evolution and economic significance of naviform core-and-blade technology in the {Southern} {Levant}},
	volume = {21},
	issn = {0153-9345},
	url = {http://www.jstor.org/stable/41492608},
	abstract = {A review of Levantine prehistory from the Epipaleolithic through the Pottery Neolithic demonstrates that naviform core-and-blade technology developed as a specialized craft in the context of the flourishing Pre-Pottery Neolithic village economy. A technological analysis reveals that the craft permitted the systematic production of standardized blade blanks to meet the specific tool needs of the Neolithic communities. The demise of the technology is linked to the socioeconomic collapse that occurred at the end of the Pre-Pottery Neolithic. Un examen de la préhistoire levantine, de l'Epipaléolithique au Néolithique avec céramique a permis de démontrer que la technologie du nucléus naviforme s'est développée en un artisanat spécialisé dans le contexte de l'économie florissante des villages du Néolithique pré-céramique. Une analyse technologique révèle que cet artisanat a permis la production systématique de supports laminaires standardisés pour répondre aux besoins d'outils spécifiques des communautés néolithiques. La disparition de cette technologie est liée à l'effondrement socio-économique de la fin du Néolithique pré-céramique.},
	number = {1},
	urldate = {2017-08-03},
	journal = {Paléorient},
	author = {QUINTERO, Leslie A. and WILKE, Philip J.},
	year = {1995},
	pages = {17--33}
}

@article{ambrose_paleolithic_2001,
	title = {Paleolithic {Technology} and {Human} {Evolution}},
	volume = {291},
	issn = {0036-8075, 1095-9203},
	url = {http://science.sciencemag.org/content/291/5509/1748},
	doi = {10.1126/science.1059487},
	abstract = {Human biological and cultural evolution are closely linked to technological innovations. Direct evidence for tool manufacture and use is absent before 2.5 million years ago (Ma), so reconstructions of australopithecine technology are based mainly on the behavior and anatomy of chimpanzees. Stone tool technology, robust australopithecines, and the genus Homo appeared almost simultaneously 2.5 Ma. Once this adaptive threshold was crossed, technological evolution was accompanied by increased brain size, population size, and geographical range. Aspects of behavior, economy, mental capacities, neurological functions, the origin of grammatical language, and social and symbolic systems have been inferred from the archaeological record of Paleolithic technology.},
	language = {en},
	number = {5509},
	urldate = {2017-08-03},
	journal = {Science},
	author = {Ambrose, Stanley H.},
	month = mar,
