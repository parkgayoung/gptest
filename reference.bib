	pmid = {21357235},
	pages = {1129--1138},
	file = {Full Text PDF:/Users/gayoungp/Zotero/storage/3K5K75BR/Collard et al. - 2011 - What drives the evolution of hunter–gatherer subsi.pdf:application/pdf;Snapshot:/Users/gayoungp/Zotero/storage/V4PVPRTF/1129.html:text/html}
}

@article{bettinger_evolutionary_1997,
