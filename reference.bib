	month = feb,
	year = {2008},
	keywords = {Late Pleistocene, Mongolia, Modern Man origins, Homo sapiensés, Pléistocène supérieur, Mongolie, Origine de l’Homme moderne, Homo sapiens},
	pages = {51--60},
	file = {ScienceDirect Full Text PDF:/Users/gayoungp/Zotero/storage/6QTPB6RP/Coppens et al. - 2008 - Discovery of an archaic Homo sapiens skullcap in N.pdf:application/pdf;ScienceDirect Snapshot:/Users/gayoungp/Zotero/storage/CVQSI43U/S1631068307001650.html:text/html}
