	keywords = {Projectile points, Bell Beaker, Geometric morphometrics, Lithic raw material, Reutilization, Shape categories},
	pages = {893--904},
	file = {Springer Full Text PDF:/Users/gayoungp/Zotero/storage/U949JK6Q/Petřík et al. - 2018 - Shape matters assessing regional variation of Bel.pdf:application/pdf}
}

