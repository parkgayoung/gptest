	file = {ScienceDirect Full Text PDF:/Users/gayoungp/Zotero/storage/EVHSQZE7/Acerbi and Alexander Bentley - 2014 - Biases in cultural transmission shape the turnover.pdf:application/pdf;ScienceDirect Snapshot:/Users/gayoungp/Zotero/storage/J3K2RP28/S1090513814000233.html:text/html}
}

@article{mesoudi_random_2009,
