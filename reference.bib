	doi = {10.1007/978-3-319-15138-0_5},
	keywords = {Archaeology, Anthropology, Migration, Pleistocene migrations, Kelp highway, Lithic technologies, Stemmed points, Coastal migration theory, Peopling of the Americas},
	pages = {49--58},
	file = {Snapshot:/Users/gayoungp/Zotero/storage/GPZQDZHR/978-3-319-15138-0_5.html:text/html}
}

@article{ulm_complexity_2013,
	series = {Peopling the last new worlds: the first colonisation of {Sahul} and the {Americas}},
	title = {‘{Complexity}’ and the {Australian} continental narrative: {Themes} in the archaeology of {Holocene} {Australia}},
	volume = {285},
	issn = {1040-6182},
	shorttitle = {‘{Complexity}’ and the {Australian} continental narrative},
	url = {http://www.sciencedirect.com/science/article/pii/S1040618212002078},
	doi = {10.1016/j.quaint.2012.03.046},
	abstract = {Accounts of long-term cultural change in Australia have emphasised the late Holocene as the period when ‘complexity’ emerged amongst foragers in Australia, associated with increased economic productivity, reduced mobility, population growth, intensified social relations and cosmological elaboration. These reconfigurations have often been interpreted as the result of continent-wide trajectories which began in the mid-Holocene, often termed ‘intensification’. These approaches have been found wanting as they homogenise diverse records of human adaptation into a single account which inexorably leads to the ethnographic present. The archaeological record tells a rather different story with fluctuating occupational intensity and even regional abandonments featuring in well-documented archaeological records. Instead, variability documented in the ethnographic and archaeological records can be understood as a product of local adaptations reflecting the operation of historically situated systems of social organisation in diverse environmental settings.},
	urldate = {2016-10-23},
	journal = {Quaternary International},
	author = {Ulm, Sean},
	month = feb,
	year = {2013},
	pages = {182--192},
	file = {ScienceDirect Full Text PDF:/Users/gayoungp/Zotero/storage/JK87GGJX/Ulm - 2013 - ‘Complexity’ and the Australian continental narrat.pdf:application/pdf;ScienceDirect Snapshot:/Users/gayoungp/Zotero/storage/BCVINTRT/S1040618212002078.html:text/html}
}

@article{morales_measuring_2013,
	title = {Measuring {Retouch} {Intensity} in {Lithic} {Tools}: {A} {New} {Proposal} {Using} 3D {Scan} {Data}},
	volume = {22},
	issn = {1072-5369, 1573-7764},
	shorttitle = {Measuring {Retouch} {Intensity} in {Lithic} {Tools}},
	url = {http://link.springer.com/article/10.1007/s10816-013-9189-0},
	doi = {10.1007/s10816-013-9189-0},
	abstract = {Identifying reduction dynamics in prehistoric retouched stone tools is important for understanding technological trends, as well as site function, raw material management and mobility of nomadic hunter-gatherers. In the absence of refits, the final state of the abandoned piece is the only remain that archaeologists have for the study of lithic reduction. The establishment of experimentally tested indexes providing strong correlations between estimated and real reduction are needed. In our work, we propose a new procedure for estimate reduction percentage. In this proposal, data are obtained from high resolution three-dimensional (3D) scans and manual measurements are avoided. The experimental test has been realized using distally retouched tools, the less suitable tools for reduction studies. The correlation levels obtained between the measured data and the estimated data increase substantially the original published from manual measurement, reaching a r2 value of 0.81 and a correlation of 0.9. The main contribution of the proposed method is the very high correlation obtained in the volume estimation (r2 = 0.89 and r = 0.94) using the 3D-based measurements. The 3D models of the unretouched and retouched flakes used in this work are free to use and accessible through an online repository.},
	language = {en},
	number = {2},
	urldate = {2016-10-25},
	journal = {Journal of Archaeological Method and Theory},
	author = {Morales, Juan I. and Lorenzo, Carlos and Vergès, Josep M.},
	month = oct,
	year = {2013},
	pages = {543--558},
	file = {Full Text PDF:/Users/gayoungp/Zotero/storage/EM6U9A4B/Morales et al. - 2013 - Measuring Retouch Intensity in Lithic Tools A New.pdf:application/pdf;Snapshot:/Users/gayoungp/Zotero/storage/AXWNBAUI/s10816-013-9189-0.html:text/html}
}

