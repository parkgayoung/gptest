	author = {Yoo, Dong G. and Kim, Seong P. and Lee, Chi W. and Chang, Tae S. and Kang, Nyeon K. and Lee, Gwang S.},
	month = sep,
	year = {2014},
	keywords = {Late Quaternary, High-resolution seismic study, Transgressive deposit, Holocene sea-level rise, Korea Strait shelf},
