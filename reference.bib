	url = {http://www.sciencedirect.com/science/article/pii/S0047248409001584},
	doi = {10.1016/j.jhevol.2009.09.002},
	abstract = {The excavations carried out in Cova Gran de Santa Linya (Southeastern PrePyrenees, Catalunya, Spain) have unearthed a new archaeological sequence attributable to the Middle Palaeoloithic/Upper Palaeolithic (MP/UP) transition. This article presents data on the stratigraphy, archaeology, and 14C AMS dates of three Early Upper Palaeolithic and four Late Middle Palaeolithic levels excavated in Cova Gran. All these archaeological levels fall within the 34–32 ka time span, the temporal frame in which major events of Neanderthal extinction took place. The earliest Early Upper Palaeolithic (497D) and the latest Middle Palaeolithic (S1B) levels in Cova Gran are separated by a sterile gap and permit pinpointing the time period in which the Mousterian disappeared from Northeastern Spain. Technological differences between the Early Upper Palaeolithic and Late Middle Palaeolithic industries in Cova Gran support a cultural rupture between the two periods. A series of 12 14C AMS dates prompts reflections on the validity of reconstructions based on radiocarbon data. Thus, results from excavations in Cova Gran lead us to discuss the scenarios relating the MP/UP transition in the Iberian Peninsula, a region considered a refuge of late Neanderthal populations.},
	number = {3},
	urldate = {2016-12-09},
	journal = {Journal of Human Evolution},
	author = {Martínez-Moreno, Jorge and Mora, Rafael and Torre, Ignacio de la},
	month = mar,
	year = {2010},
	keywords = {Cova Gran, Middle/Upper Palaeolithic transition, Archaeo-stratigraphy, 14C, Neanderthal extinction, Iberian Peninsula},
	pages = {211--226},
	file = {ScienceDirect Snapshot:/Users/gayoungp/Zotero/storage/FVIDNVZE/S0047248409001584.html:text/html}
}

@inproceedings{conard_last_2006,
	title = {The last {Neanderthals} and first modern humans in the {Swabian}},
	abstract = {Abstract. The chronostratigraphic data from Geißenklösterle and Hohle Fels suggest that the Swabian Aurignacian began about 40,000 calendar years ago. Although the human fossil evidence is sparse, Neanderthals produced the Middle Paleolithic assemblages, and we assume that modern humans produced the Aurignacian. Swabia is one of the areas in which the Aurignacian was established at an early date. Lithic and organic artifacts document a clear shift in technology between the Middle Paleolithic and the Aurignacian. With a few exceptions, the density of cultural material from Aurignacian deposits surpasses that of the Middle Paleolithic deposits. These data suggest that the intensity of occupation was higher during the Aurignacian than during the Middle Paleolithic. This paper presents the evidence for and against continuity across this boundary. An assessment of the “Danube Corridor ” and the “Population Vacuum ” models concludes that these are plausible scenarios for the beginning of the Aurignacian and end of the Middle Paleolithic. While we do not expect interpretations that apply to Swabia to apply directly to other regions, the Swabian data point to explanations for the extinction of Neanderthals and the territorial and},
